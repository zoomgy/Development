package Lec1;

public class Arithmetic_Operator {
	public static void main(String args[]) {

		int a = -4;
		System.out.println(a++); // Post Increment operator
		System.out.println(++a); // Pre Increment operator
		System.out.println(a);
		
		int x =10;
		int p = 5 + a++ - x-- -a + a++;
		System.out.println(p);
	}

}

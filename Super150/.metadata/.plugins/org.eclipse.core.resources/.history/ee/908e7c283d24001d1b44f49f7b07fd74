package Lec1;

public class Arithmetic_Operator {
	public static void main(String args[]) {

//		int a = -4;
//		int x = 10;
//		int p = 5 + a++ - x-- -a + a++;
//		System.out.println(p);
		int a = 5;
		int x = 7
//		int k = 77;
//		System.out.println(--k); // k=k-1;
		int n = a++ + ++a - --x + --x + ++x;
		System.out.println(n);
	}

}
